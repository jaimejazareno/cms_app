<% provide(:title, 'All categories') %>
<h1>All categories</h1>
<% if current_user.admin%>
  <%= link_to('New Category', new_category_path, class: "btn btn-success") %>
<% end %>
<%= will_paginate %>

<ul class="users">
  <% @categories.each do |category| %>
    <%= render category %>
  <% end %>
</ul>

<%= will_paginate %>